}

@article{RN1639,
   author = {Ansar, S. and Edvinsson, L.},
   title = {Equal contribution of increased intracranial pressure and subarachnoid blood to cerebral blood flow reduction and receptor upregulation after subarachnoid hemorrhage. Laboratory investigation},
   journal = {J Neurosurg},
   volume = {111},
   number = {5},
