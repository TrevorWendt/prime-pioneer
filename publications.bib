@article{RN1619,
   author = {Ansar, S. and Chatzikonstantinou, E. and Wistuba-Schier, A. and Mirau-Weber, S. and Fatar, M. and Hennerici, M. G. and Meairs, S.},
   title = {Characterization of a new model of thromboembolic stroke in C57 black/6J mice},
   journal = {Transl Stroke Res},
