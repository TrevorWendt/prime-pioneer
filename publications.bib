   author = {Maddahi, A. and Ansar, S. and Chen, Q. and Edvinsson, L.},
   title = {Blockade of the MEK/ERK pathway with a raf inhibitor prevents activation of pro-inflammatory mediators in cerebral arteries and reduction in cerebral blood flow after subarachnoid hemorrhage in a rat model},
